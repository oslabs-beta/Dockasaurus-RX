{
  "name": "@docker/extension-api-client-types",
  "packageManager": "yarn@3.2.0",
  "description": "Typings for the Docker Desktop extension API client",
  "version": "0.3.4",
  "keywords": [
    "docker",
    "extension"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "test:unit": "",
    "build": "tsc --build",
    "docs": "typedoc --options ./docs-generation/typedoc/typedoc.json && ts-node --esm ./docs-generation/addHeaderForJekyll/addHeaderForJekyll.ts ./docs",
    "prepack": "rimraf ./dist && tsc --build --force"
  },
  "devDependencies": {
    "@docker/eslint-config": "0.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.18.9",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.0.1",
    "globby": "^11.0.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4"
  }
}